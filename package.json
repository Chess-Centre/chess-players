{
  "name": "chess-players",
  "version": "1.0.0",
  "description": "RESTFul API Service for FIDE chess players",
  "main": "index.js",
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "mock"
    ]
  },
  "scripts": {
    "test": "mocha -R spec src/**/*.spec.js",
