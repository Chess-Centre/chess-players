  "devDependencies": {
    "eslint": ">=4.18.2",
    "mocha": "^8.1.1",
    "should": "^11.1.2",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "big-xml": "^1.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
